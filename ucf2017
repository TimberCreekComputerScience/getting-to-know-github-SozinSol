//power

import java.util.Scanner;

public class power {
	static Scanner dave = new Scanner(System.in);

	public static void main(String[] args) {

		int fool = dave.nextInt();
		for (int i = 0; i < fool; i++) {
			int a = dave.nextInt();
			System.out.println((int) Math.pow(2, a) - 1);
		}

	}

}

