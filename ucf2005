//eggs

import java.util.Scanner;

public class eggs {
	static Scanner dave = new Scanner(System.in);

	public static void main(String[] args) {

		int a = dave.nextInt();
		for (int i = 1; i <= a; i++) {
			int age = dave.nextInt();
			int b = dave.nextInt();

			if (b == 350000) {
				System.out.println("Breakfast #" + i + ": 0");
			}

			else if (age < 18) {
				System.out.println("Breakfast #" + i + ": 4");
			}

			else {
				System.out.println("Breakfast #" + i + ": 5");
			}
		}
	}
}
